{
  "name": "Passgori",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Password storage extension using Nigori.",
  "browser_action": {
    "default_icon": "nigori-logo-small-notext.png",
    "default_popup": "html/popup.html"
  },
  "permissions": [
    "https://nigori-dev.appspot.com/"
  ],
  "content_scripts": [
      {
          "matches": ["http://*/*","https://*/*"],
          "js": ["bin/pageinject.dart.js"]
      }
  ]
}